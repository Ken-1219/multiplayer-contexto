{
  "name": "contexto_multiplayer",
  "version": "0.1.0",
  "description": "A modern word guessing game inspired by Contexto, built with Next.js and AI-powered semantic similarity",
  "private": true,
  "author": {
    "name": "Contexto Multiplayer Team",
    "email": "contact@contexto-multiplayer.com"
  },
  "license": "MIT",
  "homepage": "https://contexto-multiplayer.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/contexto-multiplayer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/contexto-multiplayer/issues"
  },
  "keywords": [
    "word-game",
    "puzzle",
    "contexto",
    "semantic-similarity",
    "ai",
    "nextjs",
    "typescript",
    "tailwindcss",
    "game",
    "word-puzzle"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:check": "eslint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out build",
    "analyze": "ANALYZE=true next build",
    "build:standalone": "BUILD_STANDALONE=true next build",
    "postinstall": "husky install || true",
    "prepare": "husky install || true",
    "test": "echo \"Tests not implemented yet\" && exit 0",
    "test:watch": "echo \"Tests not implemented yet\" && exit 0",
    "test:coverage": "echo \"Tests not implemented yet\" && exit 0",
    "preview": "next build && next start",
    "deploy": "next build && next export",
    "validate": "run-s type-check lint:check format:check",
    "ci": "run-s validate build"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.896.0",
    "@aws-sdk/client-dynamodb": "^3.956.0",
    "@aws-sdk/lib-dynamodb": "^3.956.0",
    "@huggingface/inference": "^4.11.0",
    "@nextui-org/react": "^2.6.11",
    "@nextui-org/theme": "^2.4.5",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compromise": "^14.14.4",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "natural": "^8.1.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "bun@1.0.0",
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
